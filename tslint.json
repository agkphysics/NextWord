{
  "extends": "tslint:recommended",
  "rules": {
    "eofline": true,
    "no-reference": false,
    "max-line-length": [
      false
    ],
    "interface-name": [
      false,
      "never-prefix"
    ],
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-consecutive-blank-lines": false
  }
}
